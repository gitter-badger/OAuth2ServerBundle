{
    "name": "spomky-labs/oauth2-server-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 OAuth2 Server Bundle",
    "keywords": ["OAuth2", "Server", "Bundle", "Symfony2"],
    "homepage": "https://github.com/Spomky-Labs/OAuth2ServerBundle",
    "license": "MIT",
    "authors": [
        {
          "name": "Florent Morselli"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "~2.3",
        "symfony/security-bundle": "~2.3",
        "doctrine/doctrine-bundle": "~1.0",
        "matthiasnoback/symfony-bundle-plugins": "~1.0.0",
        "puli/symfony-bundle": "~1.0@beta",
        "puli/symfony-bridge": "~1.0@beta",
        "puli/repository": "~1.0@beta",
        "puli/discovery": "~1.0@beta",
        "puli/twig-extension": "~1.0@beta",
        "puli/composer-plugin": "~1.0@beta",
        "puli/url-generator": "~1.0@beta",
        "webmozart/key-value-store": "~1@beta",
        "spomky-labs/oauth2-server-password-client": "~5.0.0",
        "spomky-labs/oauth2-server-public-client": "~5.0.0",
        "spomky-labs/oauth2-server-exception-manager": "~5.0.0",
        "spomky-labs/oauth2-server-client-manager-supervisor": "~5.0.0",
        "spomky-labs/oauth2-server-scope-manager": "~5.0.0",
        "spomky-labs/oauth2-server-bearer-token": "~5.0.0",
        "spomky-labs/oauth2-server-simple-string-access-token": "~5.0.0",
        "spomky-labs/oauth2-server-authorization-endpoint": "~5.0.0",
        "spomky-labs/oauth2-server-token-endpoint": "~5.0.0",
        "spomky-labs/oauth2-server-token-revocation-endpoint": "~5.0.0",
        "spomky-labs/oauth2-server-authcode-grant-type": "~5.0.0",
        "spomky-labs/oauth2-server-implicit-grant-type": "~5.0.0",
        "spomky-labs/oauth2-server-refresh-token-grant-type": "~5.0.0",
        "spomky-labs/oauth2-server-client-credentials-grant-type": "~5.0.0",
        "spomky-labs/oauth2-server-resource-owner-password-credentials-grant-type": "~5.0.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "vipsoft/doctrine-data-fixtures-extension": "3.*@dev",
        "symfony/finder": "~2.3",
        "symfony/form": "~2.3",
        "symfony/twig-bundle": "~2.3",
        "symfony/validator": "~2.3",
        "behat/behat": "3.*",
        "behat/symfony2-extension": "2.0.x-dev",
        "behat/mink": "*",
        "behat/mink-extension": "*",
        "behat/mink-browserkit-driver":  "*",
        "doctrine/orm": ">=2.2,<2.5-dev",
        "symfony/event-dispatcher": "~2.3",
        "symfony/class-loader": "~2.3",
        "symfony/yaml": "~2.3",
        "symfony/var-dumper": "~2.3"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "SpomkyLabs\\OAuth2ServerBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpomkyLabs\\TestBundle\\": "Tests/TestBundle",
            "SpomkyLabs\\OAuth2ServerBundle\\Features\\Context\\": "Tests/Context"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
